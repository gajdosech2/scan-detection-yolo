            </Layer>
        </StateImagery>
        <StateImagery name="NoFrameEnabledBackground">
            <Layer>
                <Section look="DarkHiro/Template" section="FrameNormal"/>
            </Layer>
        </StateImagery>
        <StateImagery name="NoFrameDisabledBackground">
            <Layer>
                <Section look="DarkHiro/Template" section="FrameNormal"/>
            </Layer>
        </StateImagery>
    </WidgetLook>
    <!--
:::: Tooltip
-->
    <WidgetLook name="DarkHiro/Tooltip">
        <Property name="Alpha" value="0" />
        <Property name="Size" value="{{0, 128}, {0, 30}}" />
        <Property name="DisplayTime" value="0" />
        <Property name="HoverTime" value="0.5" />
        <NamedArea name="TextArea">
            <Area>
                <Dim type="LeftEdge" ><ImageDim name="DarkHiroImg/FrameNormalL" dimension="Width" /></Dim>
                <Dim type="TopEdge" ><ImageDim name="DarkHiroImg/FrameNormalT" dimension="Height" /></Dim>
                <Dim type="RightEdge" >
                    <OperatorDim op="Subtract">
                        <UnifiedDim scale="1" type="RightEdge" />
                        <ImageDim name="DarkHiroImg/FrameNormalR" dimension="Width" />
                    </OperatorDim>
                </Dim>
                <Dim type="BottomEdge" >
                    <OperatorDim op="Subtract">
                        <UnifiedDim scale="1" type="BottomEdge" />
                        <ImageDim name="DarkHiroImg/FrameNormalB" dimension="Height" />
                    </OperatorDim>
                </Dim>
            </Area>
        </NamedArea>
        <ImagerySection name="label">
            <TextComponent>
                <Area>
                    <Dim type="LeftEdge" ><ImageDim name="DarkHiroImg/FrameNormalL" dimension="Width" /></Dim>
                    <Dim type="TopEdge" ><ImageDim name="DarkHiroImg/FrameNormalT" dimension="Height" /></Dim>
                    <Dim type="RightEdge" >
                        <OperatorDim op="Subtract">
                            <UnifiedDim scale="1" type="RightEdge" />
                            <ImageDim name="DarkHiroImg/FrameNormalR" dimension="Width" />
                        </OperatorDim>
                    </Dim>
                    <Dim type="BottomEdge" >
                        <OperatorDim op="Subtract">
                            <UnifiedDim scale="1" type="BottomEdge" />
                            <ImageDim name="DarkHiroImg/FrameNormalB" dimension="Height" />
                        </OperatorDim>
                    </Dim>
                </Area>
                <Colours topLeft="FFF0F0F0" topRight="FFF0F0F0" bottomLeft="FFF0F0F0" bottomRight="FFF0F0F0" />
                <VertFormat type="LeftAligned" />
                <HorzFormat type="LeftAligned" />
            </TextComponent>
        </ImagerySection>
        <StateImagery name="Enabled">
            <Layer>
                <Section look="DarkHiro/Template" section="FrameWhite">
                    <Colours bottomLeft="FF435B72" bottomRight="FF435B72" topLeft="FF435B72" topRight="FF435B72"/>
                </Section>
                <Section section="label" />
            </Layer>
        </StateImagery>
        <StateImagery name="Disabled">
            <Layer>
                <Section look="DarkHiro/Template" section="FrameWhite">
                    <Colours bottomLeft="FF435B72" bottomRight="FF435B72" topLeft="FF435B72" topRight="FF435B72"/>
                </Section>
                <Section section="label" />
            </Layer>
        </StateImagery>

        <AnimationDefinition name="FadeIn" duration="0.33" replayMode="once">
            <Affector property="Alpha" interpolator="float">
                <KeyFrame position="0" sourceProperty="Alpha" />
                <KeyFrame position="0.33" value="1" progression="linear" />
            </Affector>

            <Subscription event="TooltipActive" action="Start" />
        </AnimationDefinition>

        <AnimationDefinition name="FadeOut" duration="0.33" replayMode="once">
            <Affector property="Alpha" interpolator="float">
                <KeyFrame position="0" sourceProperty="Alpha" />
                <KeyFrame position="0.33" value="0" progression="linear" />
            </Affector>
            <Affector property="Visible" interpolator="bool">
                <KeyFrame position="0" sourceProperty="Visible" />
                <KeyFrame position="0.33" value="false" progression="discrete" />
            </Affector>

            <Subscription event="TooltipInactive" action="Start" />
        </AnimationDefinition>
    </WidgetLook>

    <!--
:::: ArrayPanel
-->
    <WidgetLook name="DarkHiro/ArrayPanel">
        <Property name="WantsMultiClickEvents" value="false"/>
        <Property name="MouseInputPropagationEnabled" value="true"/>
        <Property name="ZOrderingEnabled" value="false"/>
        <Property name="Size" value="{{1, 0}, {0, 100}}"/>

        <PropertyDefinition initialValue="FF454545 " name="Color" redrawOnWrite="true" type="Colour"/>
        <PropertyDefinition initialValue="FF2D2D2D" name="ColorDisabled" redrawOnWrite="true" type="Colour"/>
        <StateImagery name="Enabled">
            <Layer>
                <Section look="DarkHiro/Template" section="BrushWhite">
                    <ColourProperty name="Color"/>
                </Section>
            </Layer>
        </StateImagery>
        <StateImagery name="Disabled">
            <Layer>
                <Section look="DarkHiro/Template" section="BrushWhite">
                    <ColourProperty name="ColorDisabled"/>
                </Section>
            </Layer>
        </StateImagery>

    </WidgetLook>

    <!--
:::: StatusIcon
-->
    <WidgetLook name="DarkHiro/StatusIcon">
        <PropertyDefinition initialValue="" name="Image" redrawOnWrite="true" type="Image"/>
        <PropertyDefinition initialValue="FFFFFFFF" name="Color" redrawOnWrite="true" type="Colour"/>
        <Property name="MouseInputPropagationEnabled" value="true"/>
        <Property name="ZOrderingEnabled" value="false"/>
        <Property name="Size" value="{{0, 20}, {0, 20}}"/>
        <ImagerySection name="glyph">
            <ImageryComponent>
                <Area>
                    <Dim type="LeftEdge">
                        <AbsoluteDim value="0"/>
                    </Dim>
                    <Dim type="TopEdge">
                        <AbsoluteDim value="0"/>
                    </Dim>
                    <Dim type="RightEdge">
                        <UnifiedDim scale="1.0" type="RightEdge"/>
                    </Dim>
                    <Dim type="BottomEdge">
                        <UnifiedDim scale="1.0" type="BottomEdge"/>
                    </Dim>
                </Area>
                <ImageProperty name="Image"/>
                <ColourProperty name="Color"/>
                <VertFormat type="CentreAligned"/>
                <HorzFormat type="CentreAligned"/>
            </ImageryComponent>
        </ImagerySection>
        <StateImagery name="Enabled">
            <Layer>
                <Section section="glyph"/>
            </Layer>
        </StateImagery>
        <StateImagery name="Disabled">
            <Layer>
                <Section section="glyph"/>
            </Layer>
        </StateImagery>
    </WidgetLook>

</Falagard>
